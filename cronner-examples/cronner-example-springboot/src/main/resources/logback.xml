<configuration scan="false"  scanPeriod="1 minutes"   debug="true">	<appender name="Console" class="ch.qos.logback.core.ConsoleAppender">		<encoder>			<pattern>%d{HH:mm:ss.SSS} %level [%logger{5}.%M:%L] %msg%n</pattern>		</encoder>	</appender>	<logger name="cronner.jfaster.org"   level="info"  additivity="false">	   <appender-ref ref="Console" />	</logger>	<root level="warn">		<appender-ref ref="Console" />	</root></configuration>